{
    "name": "daltri-menu-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "engines": {
        "node": ">=20.9.0"
    },
    "scripts": {
        "dev": "npm run dev --workspaces --if-present",
        "db:generate": "cd apps/api && npx prisma generate",
        "build": "npm run db:generate && cd apps/api && npm run build && cd ../web && npm run build",
        "start": "cd apps/web && npm start",
        "start:web": "cd apps/web && npm start",
        "start:api": "cd apps/api && npm start",
        "test": "npm run test --workspaces --if-present",
        "lint": "npm run lint --workspaces --if-present"
    },
    "devDependencies": {
        "prettier": "^3.0.0",
        "eslint": "^8.0.0",
        "typescript": "^5.0.0"
    }
}